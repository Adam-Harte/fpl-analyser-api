{
  "name": "fpl-analyser-api",
  "version": "1.0.0",
  "description": "analyser api for fpl data",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "build": "tsc",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts --ignore-pattern node_modules/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write \"src/**/*.{ts,json}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,json}\"",
    "test": "jest --watchAll=false --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --watchAll=false --runInBand"
  },
  "repository": {
    "url": "https://github.com/Adam-Harte/fpl-analyser-api.git"
  },
  "author": "Adam Harte",
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1"
  }
}
